{
  "name": "sourcemap-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node src/main.ts",
    "build": "tsc -p tsconfig.app.json",
    "inject": "sentry-cli sourcemaps inject dist",
    "push": "sentry-cli sourcemaps upload dist",
    "sentry:sourcemaps": "npm run inject && npm run push"
  },
  "dependencies": {
    "@sentry/node": "^7.92.0"
  },
  "devDependencies": {
    "@sentry/cli": "~2.24.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.0"
  }
}
